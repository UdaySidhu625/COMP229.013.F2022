<div class="formcontainer">
			
  <h3 align="center">Contact</h3>
  <p align="center">Kindly fill all the fields to contact me</p>
  
  
  <form name="registration" id="registration" align="center">
  
    <article>
        
      <form id="formOfRegistration">
          <fieldset class="Regform">
              

              <label for="firstname">First Name:</label></br>
              <input type="text" id="firstname" class="input"/></br>
              <p id="errorFirstName" class="msgError" style="color:red"></p></br>

              <label for="lastname">Last Name:</label></br>
              <input type="text" id="lastname" class="input"/></br>
              <p id="errorLastName" class="msgError" style="color:red"></p></br>

              <label for="age">Age:</label></br>
              <input type="text" id="age" class="input"/></br>
              <p id="errorAge" class="msgError" style="color:red"></p></br>

              <label for="addressOfmember">Address:</label></br>
