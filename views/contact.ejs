              <p id="errorPostalCode" class="msgError" style="color:red"></p></br>

              <label for="city">City:</label></br>
              <input type="text" id="city" class="input"/></br>
              <p id="errorOfCity" class="msgError" style="color:red"></p></br>
              
              <label for="email">Email:</label></br>
              <input type="email" id="email" class="input"/></br>
              <p id="errorEmail" class="msgError" style="color:red"></p></br>

              <label for="province">Province:</label></br>
              <input type="text" id="province" class="input"/></br>
              <p id="errorProvince" class="msgError" style="color:red"></p></br>

              <label for="passwordfirst">Query</label></br>
              <input type="password" id="passwordfirst" class="input"/></br>
              
              <p id="errorPassword" class="msgError" style="color:red"></p></br>


              <div class="buttons">
